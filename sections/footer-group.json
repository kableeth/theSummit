    "rich_text_3nnUQg": {
      "type": "rich-text",
      "blocks": {
        "heading_GgrKtf": {
          "type": "heading",
          "settings": {
            "text": "<strong>The Story</strong>",
            "heading_tag": "h1"
          }
        },
        "subheading_iBC6BJ": {
          "type": "subheading",
          "settings": {
            "text": "FOUNDED IN 1978"
          }
        },
        "richtext_8ULXUV": {
          "type": "richtext",
          "settings": {
            "content": "<p>The Summit All Seasons Market located in Kirkwood Farmers Market, is a family run business started in 1978 by Carol Mitchell. Today The Summit is owned and operated by Carols kids Dan Mitchell & Kris Clark, who are mentoring the 3rd generation, Carols grandson., Zach Clark.</p>"
          }
        }
      },
      "block_order": [
        "heading_GgrKtf",
        "subheading_iBC6BJ",
